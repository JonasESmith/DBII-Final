@model List<List<string>>

<h1>Current Enrolled Students</h1>

<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>ID</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Adress</th>
        <th>Major</th>
        <th>Phone</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var list in Model)
    {
    <tr>
        @foreach (var item in list)
        {
            <td>@item.ToString()</td>
        }

        <td> <input type="button" name="button + @list[0]" value="Edit" onclick="window.location.href('@Url.Action("EditStudent","HomeController")')"  /> </td>
        <td> <input type="button" name="delete + @list[0]" value="Delete" /> </td>
    </tr>
    }
    
</table>