@model List<Student>

<h1>Current Enrolled Students</h1>

<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Major</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.FirstName</td>
            <td>@student.LastName</td>
            <td>@student.Phone</td>
            <td>@student.Major</td>
        </tr>
    }
</table>